#: notes@maestroschan.fr/extension.js
msgid "Show notes"
msgstr "Mostrar notas"

#: notes@maestroschan.fr/extension.js
msgid "Notes extension error: failed to load a note"
msgstr "Erro na extensão: Falhou ao carregar nota"

#: notes@maestroschan.fr/extension.js notes@maestroschan.fr/prefs.ui
msgid "Notes"
msgstr "Notas"

#: notes@maestroschan.fr/extension.js
msgid "Restart the extension to apply the changes"
msgstr "Reiniciar a extensão para aplicar mudanças"

#: notes@maestroschan.fr/menus.js
msgid "Custom color"
msgstr "Cor customizada"

#: notes@maestroschan.fr/menus.js notes@maestroschan.fr/prefs.ui
msgid "Settings"
msgstr "Configurações"

